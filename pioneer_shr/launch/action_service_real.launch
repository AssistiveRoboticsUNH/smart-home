<launch> 

    <!-- start approach person service -->
    <node pkg="pioneer_shr" type="approach_person_service" name="approach_person_service" output="screen">
		<param name="predefined1PositionX" type="double" value="0.0497"/>
		<param name="predefined1PositionY" type="double" value="-0.1144"/>
		<param name="predefined1OrientationZ" type="double" value="0.7772"/>
		<param name="predefined1OrientationW" type="double" value="0.6292"/>
		<param name="predefined1Name" type="string" value="Bedroom"/>
        <param name="predefined2PositionX" type="double" value="-1.1975"/>
		<param name="predefined2PositionY" type="double" value="-1.0274"/>
		<param name="predefined2OrientationZ" type="double" value="0.9541"/>
		<param name="predefined2OrientationW" type="double" value="-0.2994"/>
		<param name="predefined2Name" type="string" value="Kitchen"/>
		<param name="targetPersonName" type="string" value="Alex"/>
		<param name="cameraFrameId" type="string" value="camera_optical_frame"/>
	</node>

    <!-- start run script service -->
    <node pkg="pioneer_shr" type="run_script_service" name="run_script_service" output="screen">
    </node>

    <!-- start DB monitor service -->
    <node pkg="pioneer_shr" type="db_monitor_service" name="db_monitor_service" output="screen">
    </node>

    <!-- start move to service -->
    <node pkg="pioneer_shr" type="move_to_service" name="move_to_service" output="screen">
		<param name="predefined1PositionX" type="double" value="0.0497"/>
		<param name="predefined1PositionY" type="double" value="-0.1144"/>
		<param name="predefined1OrientationZ" type="double" value="0.7772"/>
		<param name="predefined1OrientationW" type="double" value="0.6292"/>
		<param name="predefined1Name" type="string" value="Bedroom"/>
        <param name="predefined2PositionX" type="double" value="-1.1975"/>
		<param name="predefined2PositionY" type="double" value="-1.0274"/>
		<param name="predefined2OrientationZ" type="double" value="0.9541"/>
		<param name="predefined2OrientationW" type="double" value="-0.2994"/>
		<param name="predefined2Name" type="string" value="Kitchen"/>
        <param name="predefined3PositionX" type="double" value="-0.3334"/>
		<param name="predefined3PositionY" type="double" value="-0.3031"/>
		<param name="predefined3OrientationZ" type="double" value="-0.0527"/>
		<param name="predefined3OrientationW" type="double" value="0.9986"/>
		<param name="predefined3Name" type="string" value="door"/>
	</node>
</launch>

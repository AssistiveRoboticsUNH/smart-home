<launch> 

    <include file="$(find openni2_launch)/launch/openni2.launch">
		<arg name="rgb_frame_id" value="camera_optical_frame"/>
		<arg name="depth_frame_id" value="camera_optical_frame"/>
    </include>

    <!-- start face recognition -->
    <node pkg="face_recognition_pkg" type="recognise_multiple_faces.py" name="recognise_multiple_face" output="screen">
		<param name="cameraTopic" type="string" value="camera/rgb/image_raw"/>
		<param name="isGazebo" type="bool" value="false"/>
	</node>

</launch>
